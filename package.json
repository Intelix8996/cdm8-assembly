{
    "name": "cdm8-assembly",
    "displayName": "cdm8 assembly",
    "description": "Assembler for cdm8 processor",
    "version": "1.1.1",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
	"author": {
		"name": "Nikolay Repin"
	},
    "icon": "icon.png",
    "publisher": "intelix",
	"keywords": [
		"assembly",
		"macro",
		"assembler",
		"cdm8",
		"asm"
	],
    "contributes": {
        "languages": [{
            "id": "cdm8-assembly",
            "aliases": ["cdm8 assembly", "cdm8-assembly"],
            "extensions": [".asm .h"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cdm8-assembly",
            "scopeName": "source.asm.cdm8",
            "path": "./syntaxes/cdm8-assembly.tmLanguage.json"
        }],
        "snippets": [{
            "language": "cdm8-assembly",
            "path": "./snippets/cdm8-assembly-snippets.json"
        }]
    },
    "__metadata": {
		"publisherDisplayName": "Intelix"
	}
}